keybinds clear-defaults=true {
  shared_except "locked" {
    bind "Ctrl h" {
      MessagePlugin "https://github.com/hiasr/vim-zellij-navigator/releases/download/0.3.0/vim-zellij-navigator.wasm" {
        name "move_focus_or_tab"
        payload "left"
        move_mod "ctrl"
        use_arrow_keys "false"
      }
    }
    bind "Ctrl j" {
      MessagePlugin "https://github.com/hiasr/vim-zellij-navigator/releases/download/0.3.0/vim-zellij-navigator.wasm" {
        name "move_focus"
        payload "down"
        move_mod "ctrl"
        use_arrow_keys "false"
      }
    }
    bind "Ctrl k" {
      MessagePlugin "https://github.com/hiasr/vim-zellij-navigator/releases/download/0.3.0/vim-zellij-navigator.wasm" {
        name "move_focus"
        payload "up"
        move_mod "ctrl"
        use_arrow_keys "false"
      }
    }
    bind "Ctrl l" {
      MessagePlugin "https://github.com/hiasr/vim-zellij-navigator/releases/download/0.3.0/vim-zellij-navigator.wasm" {
        name "move_focus_or_tab"
        payload "right"
        move_mod "ctrl"
        use_arrow_keys "false"
      }
    }
  }
  scroll {
    bind "Ctrl c" {
      ScrollToBottom
      SwitchToMode "normal"
    }
    bind "J" {
      PageScrollDown
    }
    bind "K" {
      PageScrollUp
    }
    bind "Ctrl r" {
      SwitchToMode "entersearch"
      SearchInput 0
    }
    bind "esc" {
      SwitchToMode "normal"
    }
    bind "q" {
      SwitchToMode "normal"
    }
  }
  search {
    bind "left" {
      PageScrollUp
    }
    bind "right" {
      PageScrollDown
    }
    bind "Ctrl u" {
      PageScrollUp
    }
    bind "c" {
      SearchToggleOption "CaseSensitivity"
    }
    bind "Ctrl d" {
      PageScrollDown
    }
    bind "h" {
      PageScrollUp
    }
    bind "l" {
      PageScrollDown
    }
    bind "n" {
      Search "down"
    }
    bind "o" {
      SearchToggleOption "WholeWord"
    }
    bind "N" {
      Search "up"
    }
    bind "w" {
      SearchToggleOption "Wrap"
    }
  }
  session {
    bind "Ctrl o" {
      SwitchToMode "normal"
    }
  }
  shared_except "locked" "search" "renametab" "tmux" {
    bind "Ctrl a" {
      SwitchToMode "tmux"
    }
  }
  shared_among "scroll" "search" {
    bind "PageDown" {
      PageScrollDown
    }
    bind "PageUp" {
      PageScrollUp
    }
    bind "down" {
      ScrollDown
    }
    bind "up" {
      ScrollUp
    }
    bind "d" {
      HalfPageScrollDown
    }
    bind "j" {
      ScrollDown
    }
    bind "k" {
      ScrollUp
    }
    bind "u" {
      HalfPageScrollUp
    }
  }
  entersearch {
    bind "enter" {
      SwitchToMode "search"
    }
  }
  shared_among "entersearch" "search" {
    bind "esc" {
      ScrollToBottom
      SwitchToMode "scroll"
    }
  }
  renametab {
    bind "Ctrl b" {
      SwitchToMode "normal"
    }
    bind "esc" {
      UndoRenameTab
      SwitchToMode "normal"
    }
  }
  shared_among "renametab" "renamepane" {
    bind "enter" {
      SwitchToMode "normal"
    }
  }
  renamepane {
    bind "Ctrl c" {
      SwitchToMode "normal"
    }
    bind "esc" {
      UndoRenamePane
      SwitchToMode "pane"
    }
  }
  tmux {
    bind "left" {
      MoveFocus "left"
      SwitchToMode "normal"
    }
    bind "down" {
      MoveFocus "down"
      SwitchToMode "normal"
    }
    bind "up" {
      MoveFocus "up"
      SwitchToMode "normal"
    }
    bind "right" {
      MoveFocus "right"
      SwitchToMode "normal"
    }
    bind "space" {
      NextSwapLayout
    }
    bind "$" {
      LaunchOrFocusPlugin "session-manager" {
        floating true
        move_to_focused_tab true
      }
      SwitchToMode "normal"
    }
    bind "," {
      SwitchToMode "renametab"
      TabNameInput 0
    }
    bind "-" {
      NewPane "down"
      SwitchToMode "normal"
    }
    bind "." {
      SwitchToMode "renamepane"
      PaneNameInput 0
    }
    bind ":" {
      LaunchOrFocusPlugin "session-manager" {
        floating true
        move_to_focused_tab true
      }
      SwitchToMode "normal"
    }
    bind "H" {
      Resize "Increase left"
    }
    bind "J" {
      Resize "Increase down"
    }
    bind "K" {
      Resize "Increase up"
    }
    bind "L" {
      Resize "Increase right"
    }
    bind "[" {
      SwitchToMode "scroll"
    }
    bind "Ctrl b" {
      Write 2
      SwitchToMode "normal"
    }
    bind "Ctrl l" {
      Clear
      SwitchToMode "normal"
    }
    bind "c" {
      NewTab
      SwitchToMode "normal"
    }
    bind "d" {
      Detach
    }
    bind "h" {
      MoveFocus "left"
      SwitchToMode "normal"
    }
    bind "j" {
      MoveFocus "down"
      SwitchToMode "normal"
    }
    bind "k" {
      MoveFocus "up"
      SwitchToMode "normal"
    }
    bind "l" {
      MoveFocus "right"
      SwitchToMode "normal"
    }
    bind "n" {
      GoToNextTab
      SwitchToMode "normal"
    }
    bind "p" {
      GoToPreviousTab
      SwitchToMode "normal"
    }
    bind "r" {
      SwitchToMode "normal"
    }
    bind "s" {
      LaunchOrFocusPlugin "session-manager" {
        floating true
        move_to_focused_tab true
      }
      SwitchToMode "normal"
    }
    bind "t" {
      NewTab
      SwitchToMode "normal"
    }
    bind "v" {
      SwitchToMode "scroll"
    }
    bind "x" {
      CloseFocus
      SwitchToMode "normal"
    }
    bind "z" {
      ToggleFocusFullscreen
      SwitchToMode "normal"
    }
    bind "|" {
      NewPane "right"
      SwitchToMode "normal"
    }
  }
}
plugins {
  about location="zellij:about"
  compact-bar location="zellij:compact-bar"
  configuration location="zellij:configuration"
  filepicker location="zellij:strider" {
    cwd "/"
  }
  plugin-manager location="zellij:plugin-manager"
  session-manager location="zellij:session-manager"
  status-bar location="zellij:status-bar"
  strider location="zellij:strider"
  tab-bar location="zellij:tab-bar"
  welcome-screen location="zellij:session-manager" {
    welcome_screen true
  }
}
load_plugins {

}
web_client {
  font "monospace"
}
mouse_mode true
scroll_buffer_size 10000
copy_on_select true
session_serialization true
serialize_pane_viewport true
scrollback_lines_to_serialize 10000
show_startup_tips false

